import java.util.Scanner;
class First
{
public static void main(String args[])
{
int a,b,c;
System.out.println("enter three numbers to get the greatest number:");
Scanner s= new Scanner(Systm.in);
a=s.nextInt();
b=s.nextInt();
c=s.nextInt();
if(a>b&&a>c)
{
System.out.println("A is the greates number:"+a);
}
else if(b>a&&b>c)
{
System.out.println("B is the largest number:"+b);
}
else
{
System.out.println("c is the largest number:"+c);
}
}
}
